#include <iostream>
#include <ctime>
using namespace std;
 int main(){
int numb;
int guess;
int tries;60
srand(time(NULL));
numb=(rand()%100) + 1;
cout<<"************ NUMBER GUSSING GAME **************\n";
do 
    {cout<<"enter a guss number between (1-100)";
    cin>>guess;
    tries++;
    if (guess > numb)
        cout<<"too high\n";
   
   else if (guess<numb)
         cout<<" too low\n";
    else 
        cout<<"correct ! # of tries: "<<tries;}
    
   while(guess!=numb);

 





    return 0;
 }
